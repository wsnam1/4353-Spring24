      <th scope="col">Delivery Date</th>
      <th scope="col">Suggested Price per Gallon</th>
      <th scope="col">Total Amount due</th>
    </tr>
  </thead>
   <tbody class= "text-center">
     <tr>
       <th scope="row">1</th>
       <td>John Smith</td>
       <td>2</td>
